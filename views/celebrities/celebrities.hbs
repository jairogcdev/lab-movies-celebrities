<div class="d-flex flex-column align-items-center">
  <h2>Listado de celebrities</h2>
  <div class="list-group">
    {{#each allCelebrities}}
      <a
        class="list-group-item list-group-item-action"
        href="/celebrities/{{this._id}}"
      >{{this.name}}</a>
    {{/each}}
  </div>
</div>